{
  "project": {
    "name": "Task Manager API",
    "version": "2.0.0",
    "description": "RESTful API for task management",
    "author": "John Doe"
  },
  "features": [
    {
      "name": "Task CRUD",
      "description": "Create, read, update, delete tasks",
      "status": "implemented"
    },
    {
      "name": "User Authentication",
      "description": "JWT-based authentication",
      "status": "implemented"
    },
    {
      "name": "Task Categories",
      "description": "Organize tasks by categories",
      "status": "in_progress"
    },
    {
      "name": "Real-time Updates",
      "description": "WebSocket support for real-time task updates",
      "status": "planned"
    }
  ],
  "api_endpoints": [
    {
      "method": "GET",
      "path": "/api/tasks",
      "description": "Get all tasks",
      "auth_required": true
    },
    {
      "method": "POST",
      "path": "/api/tasks",
      "description": "Create new task",
      "auth_required": true
    },
    {
      "method": "PUT",
      "path": "/api/tasks/:id",
      "description": "Update task",
      "auth_required": true
    },
    {
      "method": "DELETE",
      "path": "/api/tasks/:id",
      "description": "Delete task",
      "auth_required": true
    }
  ],
  "tech_stack": {
    "language": "Python",
    "framework": "FastAPI",
    "database": "MongoDB",
    "cache": "Redis",
    "deployment": "Docker"
  },
  "configuration": {
    "database_url": "mongodb://localhost:27017/taskmanager",
    "redis_url": "redis://localhost:6379",
    "jwt_secret": "your-secret-key",
    "jwt_expiration": 3600,
    "cors_origins": ["http://localhost:3000", "https://app.example.com"]
  }
}
